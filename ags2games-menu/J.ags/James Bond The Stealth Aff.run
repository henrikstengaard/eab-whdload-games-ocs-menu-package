cd "A-Games:J/JamesBondTheStealthAffair/"
IF EXISTS ENV:whdloadargs
	whdload "JamesBondTheStealthAffair.Slave" $whdloadargs
ELSE
	whdload "JamesBondTheStealthAffair.Slave"
ENDIF

; Start AGS2 Run Post Script, if it exists
