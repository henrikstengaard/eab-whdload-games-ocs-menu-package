ENDIF

; Whdload
cd "A-Games:J/JahangirKhansWChampSquash/"
IF EXISTS ENV:whdloadargs
  whdload "JahangirKhansWChampSquash.Slave" $whdloadargs
ELSE
  whdload "JahangirKhansWChampSquash.Slave"
ENDIF

; Start AGS2 Run Post Script, if it exists
