; HstWB Menu Item Run script for AGS2
; -----------------------------------
;RunFile=A-Games:I/International3DTennis/International3DTennis.Slave

; Favourites Mode
IF EXISTS ENV:ags2favouritesmode
  execute S:AGS2Favourites "I" "International 3D Tennis"
  IF $ags2favouritesmode EQ "add"
    SKIP end
  ENDIF
  IF $ags2favouritesmode EQ "remove"
    SKIP end
  ENDIF
ENDIF

; Start AGS2 Run Pre Script, if it exists
IF EXISTS S:AGS2RunPre
  execute S:AGS2RunPre
ENDIF

; Whdload
cd "A-Games:I/International3DTennis/"
IF EXISTS ENV:whdloadargs
  whdload "International3DTennis.Slave" $whdloadargs
ELSE
  whdload "International3DTennis.Slave"
ENDIF

; Start AGS2 Run Post Script, if it exists
IF EXISTS S:AGS2RunPost
  execute S:AGS2RunPost
ENDIF

; End
LAB end
