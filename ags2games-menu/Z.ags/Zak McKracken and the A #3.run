cd "A-Games:Z/ZakMcKrackenFr/"
IF EXISTS ENV:whdloadargs
	whdload "ZakMcKrackenFr.slave" $whdloadargs
ELSE
	whdload "ZakMcKrackenFr.slave"
ENDIF

; Start AGS2 Run Post Script, if it exists
